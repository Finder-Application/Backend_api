{
  "name": "awesome-nestjs-boilerplate",
  "description": "Awesome NestJS Boilerplate, Typescript, Postgres, TypeORM",
  "author": "Cinny",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "build:docker": "nest build --webpack --webpackPath ./webpack.config.js && rimraf dist/tsconfig.build.tsbuildinfo",
    "build:prod": "tsc -p tsconfig.build.json",
    "postbuild:prod": "copyfiles --up 1 src/**/*.json dist",
    "start:hmr": "node dist/main.hmr.ts",
    "start:dev": "ts-node src/main.ts",
    "start:prod": "node dist/main.js",
    "dev": "nest start --watch",
    "new": "hygen new",
    "schema:drop": "yarn run typeorm schema:drop",
    "watch:dev": "ts-node-dev src/main.ts",
    "debug:dev": "cross-env TS_NODE_CACHE=false ts-node-dev --inspect --ignore '/^src/.*\\.spec\\.ts$/' src/main.ts",
    "webpack": "webpack --config webpack.config.js --progress",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint --fix . --ext .ts",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:cov": "NODE_ENV=test jest --coverage",
    "test:debug": "NODE_ENV=test node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand",
    "copy:lib": "cpx 'src/lib/**/{files,workspace}/**/*.*' dist/lib && cpx 'src/lib/**/{files,workspace}/**/.!(gitignore)' dist/lib",
    "test:e2e": "NODE_ENV=test jest --config ./test/jest-e2e.json",
    "docs:dev": "vuepress dev -p 7070",
    "docs:build": "DEPLOY_ENV=gh-pages vuepress build",
    "docs:deploy": "yarn docs:build && gh-pages -d .vuepress/dist",
    "prepare": "husky install",
    "prerelease": "yarn docs:deploy",
    "release": "release-it",
    "generator:db": "./generator_db.sh"
  },
  "dependencies": {
    "@nestjs-modules/ioredis": "^1.0.1",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^9.3.9",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.11",
    "@nestjs/cqrs": "^9.0.1",
    "@nestjs/event-emitter": "^1.3.1",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/platform-socket.io": "^9.1.2",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/terminus": "^9.1.1",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "^9.1.2",
    "@supabase/supabase-js": "^2.0.0",
    "@types/multer": "^1.4.7",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "express-ctx": "^0.1.1",
    "express-rate-limit": "^6.6.0",
    "firebase-admin": "^11.1.0",
    "generate-package-json-webpack-plugin": "^2.6.0",
    "generate-password": "^1.7.0",
    "google-auth-library": "^8.5.2",
    "googleapis": "^109.0.1",
    "helmet": "^6.0.0",
    "ioredis": "^5.2.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "morgan": "^1.10.0",
    "nats": "^2.8.0",
    "ncc": "^0.3.6",
    "nestjs-i18n": "^9.1.10",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.7",
    "socket.io": "^4.6.1",
    "source-map-support": "^0.5.21",
    "swagger-ui-express": "^4.5.0",
    "ts-mailgun": "^0.5.1",
    "typeorm": "^0.3.9",
    "uuid": "^9.0.0",
    "webpack-cli": "^5.0.1"
  },
  "devDependencies": {
    "@handfish/hygen": "^6.1.6",
    "@nestjs/cli": "^9.1.3",
    "@nestjs/platform-express": "^9.3.12",
    "@nestjs/testing": "^9.0.11",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.1.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.185",
    "@types/mime-types": "^2.1.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "copyfiles": "^2.4.1",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.17.12",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-import-helpers": "^1.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-sonarjs": "^0.16.0",
    "eslint-plugin-unicorn": "^44.0.2",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^29.2.0",
    "lint-staged": "~13.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.1.1",
    "release-it": "^15.4.2",
    "supertest": "^6.2.4",
    "terser-webpack-plugin": "^5.3.7",
    "ts-jest": "^29.0.1",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "typeorm-model-generator": "^0.4.6",
    "typescript": "^4.8.3",
    "vite": "^4.2.1",
    "webpack": "^5.76.3",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
